
<Container>
  <Name>Unifi-11notes</Name>
  <Repository>11notes/unifi:9.3.45-unraid</Repository>
  <Registry>https://github.com/11notes/docker-unifi</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/11notes/docker-unifi/issues</Support>
  <Project>https://github.com/11notes/docker-unifi</Project>
  <Overview>Custom Docker build of Unifi Controller by 11notes.
  
  Port 8443 for Dashboard

  Will add more ports to the Template when I get time but heres the list for all the required ports.
  https://help.ui.com/hc/en-us/articles/218506997-Required-Ports-Reference
  </Overview>
  <Category>Network:Management</Category>
  <WebUI>https://[IP]:8443</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/RobertCajun/unraid-templates/main/icons/unifi-11n-icon.png</Icon>

  <!-- Volume Mappings -->
  <Config Name="VAR Directory" Target="/unifi/var" Default="/mnt/user/appdata/unifi-11n/var" Mode="rw" Description="VAR folder" Type="Path" Display="always" Required="true" Mask="false"/>

  <!-- Port Mappings -->
  <Config Name="Dashboard (TCP)" Target="8443" Default="8443" Mode="tcp" Description="Dashboard TCP port" Type="Port" Display="always" Required="true" Mask="false"/>
  <Config Name="L2 Discovery Port (UDP)" Target="1900" Default="1900" Mode="udp" Description="Used with Make application discoverable on L2 network in the UniFi Network settings." Type="Port" Display="always" Required="false" Mask="false"/>
  <Config Name="STUN Port (UDP)" Target="3478" Default="3478" Mode="udp" Description="STUN UDP port" Type="Port" Display="always" Required="true" Mask="false"/>  
  <Config Name="Unifi Speed Test (TCP)" Target="6789" Default="6789" Mode="tcp" Description="User for Mobile Speed Test" Type="Port" Display="always" Required="true" Mask="false"/>   <Config Name="STUN Port (UDP)" Target="3478" Default="3478" Mode="udp" Description="STUN UDP port" Type="Port" Display="always" Required="true" Mask="false"/>
  <Config Name="Communication Port (TCP)" Target="8080" Default="8080" Mode="tcp" Description="Used for device and application communication." Type="Port" Display="always" Required="true" Mask="false"/>
  <Config Name="Device Adoption (UDP)" Target="10001" Default="10001" Mode="udp" Description="Device discovery during adoption port" Type="Port" Display="always" Required="true" Mask="false"/>

</Container>
